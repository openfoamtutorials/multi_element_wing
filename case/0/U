FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (9 0 0);

boundaryField
{
    inlet
    {
        type            freestream;
        freestreamValue $internalField;
    }

    outlet
    {
        type            freestream;
        freestreamValue $internalField;
    }

    walls
    {
        type            noSlip;
    }

    airfoil
    {
        type            noSlip;
    }

    frontAndBack
    {
        type            empty;
    }
}
